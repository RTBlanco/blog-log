<div>
  <% @posts.each do |post|%>
    <h1><%= post.title %></h1>
    <h3>By: <%= post.user.username %></h3>
    <p><%= post.content %></p>

    <% if logged_in? && post.user.id == current_user.id %>
      <a href=""><button>Edit</button></a>
    <% end %>
    <% if logged_in? && post.user.id != current_user.id %>
      <button>Like</button>
    <% end %>

  <% end %>
</div>